<?xml version="1.0" encoding="UTF-8"?>    
<!DOCTYPE generatorConfiguration  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"    
      "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">  
<generatorConfiguration>  

    <context id="mysql" targetRuntime="MyBatis3">

        <property name="suppressTypeWarnings" value="true" />


        <!-- generate entity时，生成serialVersionUID -->
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />

        <!-- 这个插件只会增加字符串字段映射到一个JDBC字符的方法 -->
        <plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />

    <!-- 抑制生成代码的注释 -->  
        <commentGenerator>
            <property name="suppressAllComments" value="true" />  
        </commentGenerator>
  
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"  connectionURL="jdbc:mysql://127.0.0.1:3306/hsdb"  userId="root" password=""> </jdbcConnection>
  
        <javaTypeResolver>  
            <property name="forceBigDecimals" value="false" />  
        </javaTypeResolver>  
  
        <!-- generate Model -->  
        <javaModelGenerator targetPackage="hs"  targetProject="temp">
            <property name="enableSubPackages" value="true" />  
            <property name="trimStrings" value="true" />  
        </javaModelGenerator>  
  
        <!-- generate xml -->  
        <sqlMapGenerator targetPackage="hs"  targetProject="temp">
            <property name="enableSubPackages" value="true" />  
        </sqlMapGenerator>  
  
        <!--生成dao接口的配置 -->  
        <javaClientGenerator type="XMLMAPPER"  targetPackage="hs" targetProject="temp"> <property name="enableSubPackages" value="true" />
        </javaClientGenerator>
        <table tableName="hs_user" domainObjectName="User" enableCountByExample="false" enableSelectByExample="false" enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false" >
            <property name="useActualColumnNames" value="true"/>
        </table>
    </context>  
</generatorConfiguration>
